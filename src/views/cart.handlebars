<h1>Cart</h1>
<hr>
<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">Title</th>
      <th scope="col">Price</th>
      <th scope="col">Stock</th>
      <th scope="col"></th>
    </tr>
  </thead>
  {{#each productsInCart}}
  <tbody>
    <tr>
      <th scope="row"></th>
      <td>{{id.title}}</td>
      <td>{{id.price}}</td>
      <td>{{id.stock}}</td>
      <td><button id="addToCart" type="button" class="btn btn-danger">X</button></td>
    </tr>
  </tbody>
  {{/each}}
</table>
